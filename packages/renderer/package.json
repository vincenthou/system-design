{
  "name": "@system-design/renderer",
  "version": "0.0.1",
  "description": "System designer renderer to render output from editor",
  "packageManager": "pnpm@7.9.2",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "pnpm-install": "pnpm install",
    "dev": "tsup --watch --format=cjs,esm",
    "build": "cross-env NODE_ENV=production tsup --format=cjs,esm",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "system design",
    "renderer"
  ],
  "author": "vincenthou",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.7",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@unocss/preset-attributify": "^0.45.26",
    "react": "^18.2.0"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,json}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "b-tween": "^0.3.3",
    "lodash-es": "^4.17.21"
  }
}
